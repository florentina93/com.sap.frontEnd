<core:View
	controllerName="sap.ui.demo.myFiori.view.Detail"
	xmlns="sap.m"
	xmlns:core="sap.ui.core" >
	<Page
		id="Detail"
		title="{i18n>DetailTitle}"
		showNavButton="{device>/isPhone}"
		navButtonPress="handleNavButtonPress" >
		<VBox>
			<Text text="{FriendName}" />
			<Text text="{FriendAddress}" />
			<Text text="{FriendAge}" />
			<Text text="{FriendWorkPlace}" />
		</VBox>
		
		<VBox>
		<HBox>
		<Label text="Name:"/>
		<Label text="Name Here" id="nametext"/>
		</HBox>
		<HBox>
		<Label text="First Name:"/>
		<Label text="Firstname Here" id="firstnametext"/>
		</HBox>
		<HBox>
		<Label text="Hometown:"/>
		<Label text="hometownHere" id="hometowntext"/>
		</HBox>
		<HBox>
		<Label text="Id:"/>
		<Label text="Id Here" id="idtext"/>
		</HBox>
		<HBox>
		<Label text="Birthday:"/>
		<Label text="Birthday Here" id="birthdaytext"/>
		</HBox>
		<HBox>
		<Label text="Gender:"/>
		<Label text="Gender Here" id="gendertext"/>
		</HBox>
		<HBox>
		<Label text="Sports:"/>
		<Label text="Sports Here" id="sportstext"/>
		</HBox>
		<HBox>
		<Label text="Work:"/>
		<Label text="Work Here" id="worktext"/>
		</HBox>
		<HBox>
		<Label text="Email:"/>
		<Label text="Email Here" id="emailtext"/>
		</HBox>
		</VBox>
	</Page>
</core:View>